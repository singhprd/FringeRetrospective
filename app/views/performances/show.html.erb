<table class='table table-condensed'>
  <thead>
    <tr>
      <th>Date</th>
      <th>Start Time</th>
      <th>End Time</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @event.performances.each do |performance| %>
      <tr>
        <td><%= fringe_time_date(performance) %></td>
        <td><%= fringe_start_time(performance) %></td>
        <td><%= fringe_end_time(performance) %></td>
      </tr>
      <% end %>
    </tr>
  </tbody>
</table>
