<p id="notice"><%= notice %></p>

<h2><%= @list.name %></h2>

<%= react_component("List", props:{
    listId: @list.id,
    listItems: @list.list_items,
    events: @list.events.map{|event| event.card_json(current_user) },
  }) %>

<div class=' col-xs-12'>
  <%= link_to 'Back', lists_path %> |
  <%= link_to 'Edit', edit_list_path(@list) %> |
  <%= link_to "Delete", list_path(@list), method: :delete, data: { confirm: 'Are you certain you want to delete this?' } %>
</div>
